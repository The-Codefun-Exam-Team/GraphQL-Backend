enum CodefunScoreType{
    "Partial scoring"
    oi,
    "ICPC rules (Accepted on all tests)"
    acm
}
type CodefunProblem{
    id: Int!
    name: String!
    pcode: String!
    setter: CodefunUser!
    
    score: Int!
    scoretype: CodefunScoreType!

    pgroup: String!
    statement: String!
    timelimit: Float!

    solved: Int!
    attempted: Int!
}
